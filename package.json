    "prop-types": "^15.5.8",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "4.1.1",
    "react-router-redux": "4.0.8",
    "react-titlebar-osx": "1.1.0",
